{
    "name": "toneflix-code/fakerphp-lorem-toneflix",
    "description": "An alternative image provider for fakerphp using Lorem Toneflix.",
    "keywords": [
        "faker",
        "php",
        "images",
        "laravel",
        "generator",
        "placeholder",
        "faker",
        "lorem"
    ],
    "homepage": "https://github.com/toneflix/fakerphp-lorem-toneflix",
    "license": "MIT",
    "authors": [
        {
            "name": "Legacy",
            "email": "code@toneflix.com.ng",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|8.2|8.3",
        "fakerphp/faker": "^1.10|^1.20"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "suggest": {
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "ToneflixCode\\FakerLoremToneflix\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ToneflixCode\\FakerLoremToneflix\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
